 "css": "@media screen {\r\n    .has-variables{\r\n        display: none;\r\n    }\r\n}\r\n@media print {\r\n    .has-variables{\r\n        display: block; }\r\n}\r\n\r\n#qr.fixed {\r\n    position: fixed;\r\n    top: 20%;\r\n    left: 10%;\r\n    width: 180px;\r\n    height: 180px;\r\n}\r\n\r\n.qr-code {\r\n    width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.terms-section{\r\n    font-size: 13px;\r\n }\r\n \r\n.signature-section {\r\n    margin-top: 50px;\r\n    margin-bottom: 20px;\r\n    position: relative;\r\n    font-weight: bold;\r\n}\r\n\r\n.signature-line {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n}\r\n\r\n.dotted-line {\r\n    display: inline-block;\r\n    width: 200px;\r\n    border-bottom: 1px dotted #000;\r\n    margin-right: 10px;\r\n}\r\n\r\n.label-right {\r\n    text-align: right;\r\n    font-weight: bold;\r\n}\r\n.label-left {\r\n    font-weight: bold;\r\n}",
