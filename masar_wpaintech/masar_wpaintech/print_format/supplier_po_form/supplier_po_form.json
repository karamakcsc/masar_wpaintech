 "doctype": "Print Format",
 "font_size": 14,
 "html": "\r\n<div class=\"page-break\">\r\n    <div class=\"print-heading\">\r\n        <h2>Purchase Order<br>\r\n            <small>{{doc.name}}</small>\r\n        </h2>\r\n    </div>\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Supplier Name</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.supplier_name}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Reference Number</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.custom_ref_number}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Date</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{ frappe.utils.formatdate(doc.transaction_date) }}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <table class=\"table table-bordered\">\r\n        <tbody>\r\n            <tr>\r\n                <th>Sr</th>\r\n                <th>Item Code</th>\r\n                <th>Supplier Code</th>\r\n                <th style=\"width: 30%;\">Description</th>\r\n                <th>UOM</th>\r\n                <th>Quantity</th>\r\n            </tr>\r\n            {% for row in doc.items %}\r\n                <tr>\r\n                    <td>{{ row.idx }}</td>\r\n                    <td>{{ row.item_code }}</td>\r\n                    <td>{{ row.custom_manufacturing_code }}</td>\r\n                    <td style=\"width: 30%;\">{{ row.description }}</td>\r\n                    <td>{{ row.uom or row.stock_uom }}</td>\r\n                    <td>{{ row.qty | int }}</td>\r\n                </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n</div>\r\n",
