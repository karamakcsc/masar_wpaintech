 "css": "@media print {\n        @page {\n            @bottom-center {\n                    content: \"Page \" counter(page);\n                    font-size: 12px;\n                    margin-bottom:120px\n                }\n            size: landscape;\n        }\n    }\n",
 "custom_format": 1,
 "default_print_language": "en-US",
