 "doc_type": "Purchase Receipt",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div class=\"page-break\">\r\n    <div class=\"print-heading\">\r\n        <div class=\"col-xs-3 column-break\">\r\n            <h4>Landed Cost</h4>\r\n        </div>\r\n        <h2>Purchase Receipt<br>\r\n            <small>{{doc.name}}</small>\r\n        </h2>\r\n    </div>\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Supplier Name</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.supplier_name}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Reference Number</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.custom_ref_number}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Order Date:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{frappe.utils.formatdate(doc.posting_date)}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Print Date:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{frappe.utils.formatdate(frappe.utils.formatdate(frappe.utils.nowdate()))}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <table class=\"table table-bordered\">\r\n        <tbody>\r\n            <tr>\r\n                <th>Sr</th>\r\n                <th>Item Code</th>\r\n                <th>Supplier Code</th>\r\n                <th style=\"width: 30%;\">Description</th>\r\n                <th>UOM</th>\r\n                <th>Quantity</th>\r\n                <th>Rate (EUR)</th>\r\n                <th>Rate</th>\r\n                <th>Amount (EUR)</th>\r\n                <th>Amount</th>\r\n                <th>Landed Cost Amount</th>\r\n                <th>Cost Per Unit</th>\r\n            </tr>\r\n            {% set cost_per_unit = namespace(value=0) %}\r\n            {% for row in doc.items | sort(attribute='item_code') %}\r\n                {% set cost_per_unit.value = (row.base_amount + row.landed_cost_voucher_amount) / row.qty %}\r\n                <tr>\r\n                    <td>{{ row.idx }}</td>\r\n                    <td>{{ row.item_code }}</td>\r\n                    <td>{{ row.custom_manufacturing_code }}</td>\r\n                    <td>{{ row.description }}</td>\r\n                    <td>{{ row.uom or row.stock_uom }}</td>\r\n                    <td>{{ row.qty | int }}</td>\r\n                    <td>{{ row.get_formatted(\"rate\", doc) }}</td>\r\n                    <td>{{ row.get_formatted(\"base_rate\", doc) }}</td>\r\n                    <td>{{ row.get_formatted(\"amount\", doc) }}</td>\r\n                    <td>{{ row.get_formatted(\"base_amount\", doc) }}</td>\r\n                    <td>{{ row.get_formatted(\"landed_cost_voucher_amount\", doc) }}</td>\r\n                    <td>{{ cost_per_unit.value | round(3)}}</td>\r\n                </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\"></div>\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total (EUR)</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.get_formatted(\"total\", doc)}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.get_formatted(\"base_total\", doc)}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total Landed Cost Amount:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.get_formatted(\"custom_total_landed_cost_amount\", doc)}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total Cost</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{ frappe.format_value(doc.base_total + doc.custom_total_landed_cost_amount, {\"fieldtype\": \"Currency\", \"options\": doc.currency}) }}\r\n                </div>\r\n            </div>\r\n            \r\n        </div>\r\n    </div>\r\n</div>\r\n",
