 "doctype": "Print Format",
 "font_size": 14,
 "html": "<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<div style=\"display: block;\">\r\n<div  style=\"width: 40%; float: left; display: inline-block\">\r\n{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n    {% if letter_footer and not no_letterfooter %}\r\n    <div class=\"letter-head\">{{ letter_footer }}</div>\r\n    {% endif %}\r\n    </div>\r\n    <div style=\"width: 50%; float: right; diplay: inline-block;\">\r\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\r\n    <div class=\"alert alert-info text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\r\n    {%- endif -%}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\r\n    <div class=\"alert alert-danger text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\r\n    {%- endif -%}\r\n    {% if max_pages > 1 %}\r\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\r\n    {% endif %}\r\n{%- endmacro -%}\r\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\r\n<label>\r\n\r\n    <div class=\"row\">\r\n        <div class=\"text-center\" style=\"font-size:20px;\">\r\n            <label>\r\n                \u0639\u0631\u0636 \u0633\u0639\u0631\r\n                <br>\r\n                Quotation\r\n            </label>\r\n        </div>\r\n    </div>\r\n\r\n\r\n</label>\r\n\r\n<table class=\"table table-bordered\" style=\"font-size: 10px;\">\r\n    <tr>\r\n        <td style=\"width: 20%;\">\r\n            {{ _(\"Date\") }}\r\n        </td>\r\n        <td style=\"width: 30%;\">\r\n            {{doc.get_formatted(\"transaction_date\") or ''}}\r\n        </td>\r\n    </tr>\r\n        <tr>\r\n        <td>\r\n            {{ _(\"Quotaion #\") }}\r\n        </td>\r\n        <td>\r\n            {{doc.name or ''}}\r\n        </td>\r\n    </tr>\r\n        <tr>\r\n        <td>\r\n            {{ _(\"Sales Tax No.\") }}\r\n        </td>\r\n        <td>\r\n            {% set tax_no = frappe.get_doc(\"Company\", doc.company) %}\r\n            {{ tax_no.tax_id or '' }}\r\n        </td>\r\n    </tr>\r\n        <tr>\r\n        <td>\r\n            {{ _(\"Customer Ref.\") }}\r\n        </td>\r\n        <td>\r\n            {%- set cust_doc = frappe.get_doc(\"Customer\", doc.party_name) -%}\r\n            {{cust_doc.custom_ref_no or ''}}\r\n        </td>\r\n    </tr>\r\n        <tr>\r\n        <td>\r\n            {{ _(\"Customer Name\") }}\r\n        </td>\r\n        <td>\r\n            {{doc.custom_customer_name_en or doc.customer_name}}\r\n        </td>\r\n    </tr>\r\n</table>\r\n</div>\r\n</div>\r\n\r\n<table class=\"table table-bordered\" style=\"font-size: 10px;\">\r\n\t<tbody>\r\n\t\t<tr>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Item Code\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Description\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"UOM\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Quantity\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Unit Price\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Amount\") }}</th>\r\n\t\t</tr>\r\n\t\t{%- for row in doc.items -%}\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 20%; text-align: center;\">\r\n\t\t\t\t<div style=\"border: 0px;\">{{ row.item_code or '' }}</div></td>\t\r\n\t\t\t<td style=\"width: 42.5%;\">\r\n\t\t\t\t<div style=\"border: 20px;\">{{ row.description or '' }}</div></td>\r\n\t\t\t<td style=\"width: 10%; text-align: center;\">\r\n\t\t\t\t<div style=\"border: 0px;\">{{row.uom or ''}}</div></td>\r\n\t\t\t<td style=\"width: 7.5%; text-align: center;\">\r\n\t\t\t\t<div style=\"border: 0px;\">{{row.qty | int or ''}}</div></td>\r\n\t\t\t<td style=\"width: 10%; text-align: right;\">{{row.get_formatted(\"rate\", doc) or '' }}</td>\r\n\t\t\t<td style=\"width: 12.5%; text-align: right;\">{{\r\n\t\t\t\trow.get_formatted(\"amount\", doc) or ''}}</td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t\t\t</tbody>\r\n\r\n\t\t<tr>\r\n\t\t    <td colspan=\"4\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        Sub-Total\r\n\t\t    </td>\r\n\t\t    <td colspan=\"3\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t         {{doc.get_formatted(\"total\") or ''}}\r\n\t\t    </td>\r\n\t\t</tr>\r\n\t\t<tr>\r\n\t\t    <td colspan=\"4\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        Discount\r\n\t\t    </td>\r\n\t\t      \r\n\t\t    <td colspan=\"3\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t         {{doc.get_formatted(\"discount_amount\") or ''}}\r\n\t\t    </td>\r\n\t\t</tr>\r\n\t\t\r\n\t\t\t\t<tr>\r\n\t\t    <td colspan=\"4\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        Net Total\r\n\t\t    </td>\r\n\t\t      \r\n\t\t    <td colspan=\"3\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t         {{doc.get_formatted(\"net_total\") or ''}}\r\n\t\t    </td>\r\n\t\t</tr>\r\n\t\t\r\n\t\t<tr>\r\n\t\t    <td colspan=\"1\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        TAX RATE &nbsp;\r\n\t\t        {%- for row in doc.taxes -%}\r\n\t\t         {{row.get_formatted( \"rate\",doc) or ''}}\r\n\t\t        {%- endfor -%}\r\n\t\t    </td>\r\n\t\t    <td colspan=\"3\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        SALES TAX\r\n\t\t    </td>\r\n\t\t       {%- for row in doc.taxes -%}\r\n\t\t    <td colspan=\"2\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t         {{row.get_formatted(\"tax_amount\",doc) or \"00.00\"}}\r\n\t\t        {%- endfor -%}\r\n\t\t    </td>\r\n\t\t</tr>\t\t\r\n\t\t\r\n\t\t\r\n\t\t<tr style=\"background-color:  #eee;\">\r\n\t\t    <td colspan=\"4\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        TOTAL\r\n\t\t    </td>\r\n\t\t    <td colspan=\"3\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t       {{doc.get_formatted(\"grand_total\") or ''}}\r\n\t\t    </td>\r\n\t\t</tr>\r\n\t\t<tr style=\"background-color:  #eee;\">\r\n\t\t    <td colspan=\"1\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t        IN WORDS\r\n\t\t    </td>\r\n\t\t    <td colspan=\"5\" style=\"font-weight: bold;text-align: right;\">\r\n\t\t       {{doc.in_words}}\r\n\t\t    </td>\r\n\t\t</tr>\r\n</table>\r\n<div class=\"page-break\"></div>\r\n\r\n{%- macro add_footer(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n{% if letter_head and not no_letterhead %}\r\n       <div class=\"footer-html\" style=\"position: fixed; left: 0; bottom: 2;width: 100%; text-align: center;\">{{ footer }}</div>\r\n       {% endif %}\r\n       \r\n {%- endmacro -%}\r\n",
