 "css": "@media screen {\r\n    .has-variables{\r\n        display: none;\r\n    }\r\n}\r\n@media print {\r\n    .has-variables{\r\n        display: block; }\r\n}\r\n\r\n\r\n.label-right {\r\n    text-align: right;\r\n    font-weight: bold;\r\n}\r\n.label-left {\r\n    font-weight: bold;\r\n}",
 "custom_format": 1,
