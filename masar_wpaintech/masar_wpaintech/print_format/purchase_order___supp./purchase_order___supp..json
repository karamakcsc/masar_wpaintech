 "html": "<div class=\"page-break\">\r\n    <div class=\"print-heading\">\r\n        <h2>Purchase Order<br>\r\n            <small>{{doc.name}}</small>\r\n        </h2>\r\n    </div>\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Supplier Name</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.supplier_name}}\r\n                </div>\r\n            </div>\r\n            {% if doc.address_display %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Address</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.address_display}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n            {% if doc.contact_display %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Contact</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.contact_display}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n            {% if doc.contact_mobile %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Mobile No</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.contact_mobile}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n        </div>\r\n\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Date</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.transaction_date}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Customer Name</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.customer_name}}\r\n                </div>\r\n            </div>\r\n            {% if doc.shipping_address_display %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Customer Address</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.shipping_address_display}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n            {% if doc.customer_contact_display %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Customer Contact</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.customer_contact_display}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n            {% if doc.customer_contact_mobile %}\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Customer Mobile No</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.customer_contact_mobile}}\r\n                </div>\r\n            </div>\r\n            {% endif %}\r\n        </div>\r\n    </div>\r\n\r\n    <table class=\"table table-bordered\">\r\n        <tbody>\r\n            <tr>\r\n                <th>Sr</th>\r\n                <th>Item Code</th>\r\n                <th>Supplier Code</th>\r\n                <th style=\"width: 30%;\">Description</th>\r\n                <th>UOM</th>\r\n                <th>Quantity</th>\r\n                <th>Qty/CRT</th>\r\n                <th>CRT No</th>\r\n                <th>WT/Unit</th>\r\n                <th>Total WT.</th>\r\n            </tr>\r\n            {% set total_cartons = namespace(value=0) %}\r\n            {% set total_weight = namespace(value=0) %}\r\n            {% for row in doc.items %}\r\n                {% set carton_no = row.custom_no_cartron | default(0) | int %}\r\n                {% set weight_total = row.total_weight | default(0) | float %}\r\n                {% set total_cartons.value = total_cartons.value + carton_no %}\r\n                {% set total_weight.value = total_weight.value + weight_total %}\r\n                <tr>\r\n                    <td>{{ row.idx }}</td>\r\n                    <td>{{ row.item_code }}</td>\r\n                    <td>{{ row.custom_manufacturing_code }}</td>\r\n                    <td style=\"width: 30%;\">{{ row.description }}</td>\r\n                    <td>{{ row.uom or row.stock_uom }}</td>\r\n                    <td>{{ row.qty }}</td>\r\n                    <td>{{ row.custom_carton_capacity }}</td>\r\n                    <td>{{ row.custom_no_cartron }}</td>\r\n                    <td>{{ row.weight_per_unit }}</td>\r\n                    <td>{{ row.total_weight | round(3)}}</td>\r\n                </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\"></div>\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.total}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Grand Total</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.grand_total}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>In Words</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.in_words}}\r\n                </div>\r\n            </div>\r\n            <br>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total CRT:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{total_cartons.value}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total Gr. WT:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{total_weight.value | round(3)}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
