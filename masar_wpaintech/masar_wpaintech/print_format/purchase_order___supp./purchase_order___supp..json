 "doc_type": "Purchase Order",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div class=\"page-break\">\r\n    <div class=\"print-heading\">\r\n        <div class=\"col-xs-3 column-break\">\r\n            <h4>Supplier</h4>\r\n        </div>\r\n        <h2>Purchase Order<br>\r\n            <small>{{doc.name}}</small>\r\n        </h2>\r\n    </div>\r\n    \r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Supplier Name</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.supplier_name}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Reference Number</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{doc.custom_ref_number}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Date</label>\r\n                </div>\r\n                <div class=\"col-xs-7 value\">\r\n                    {{frappe.utils.formatdate(doc.transaction_date)}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <table class=\"table table-bordered\">\r\n        <tbody>\r\n            <tr>\r\n                <th>Sr</th>\r\n                <th>Item Code</th>\r\n                <th>Supplier Code</th>\r\n                <th style=\"width: 30%;\">Description</th>\r\n                <th>UOM</th>\r\n                <th>Quantity</th>\r\n                <th>Qty/CRT</th>\r\n                <th>CRT No</th>\r\n                <th>WT/Unit</th>\r\n                <th>Total WT.</th>\r\n            </tr>\r\n            {% set total_cartons = namespace(value=0) %}\r\n            {% set total_weight = namespace(value=0) %}\r\n            {% for row in doc.items | sort(attribute='item_code') %}\r\n                {% set carton_no = row.custom_no_cartron | default(0) | float %}\r\n                {% set weight_total = row.total_weight | default(0) | float %}\r\n                {% set total_cartons.value = total_cartons.value + carton_no %}\r\n                {% set total_weight.value = total_weight.value + weight_total %}\r\n                <tr>\r\n                    <td>{{ row.idx }}</td>\r\n                    <td>{{ row.item_code }}</td>\r\n                    <td>{{ row.custom_manufacturing_code }}</td>\r\n                    <td style=\"width: 30%;\">{{ row.description }}</td>\r\n                    <td>{{ row.uom or row.stock_uom }}</td>\r\n                    <td>{{ row.qty | int }}</td>\r\n                    <td>{{ row.custom_carton_capacity | int }}</td>\r\n                    <td>{{ row.custom_no_cartron }}</td>\r\n                    <td>{{ row.weight_per_unit }}</td>\r\n                    <td>{{ row.total_weight | round(3)}}</td>\r\n                </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n    <div class=\"row section-break\">\r\n        <div class=\"col-xs-6 column-break\"></div>\r\n        <div class=\"col-xs-6 column-break\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{doc.total}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total CRT:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{total_cartons.value}}\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-5 text-right\">\r\n                    <label>Total Gr. WT:</label>\r\n                </div>\r\n                <div class=\"col-xs-7 text-right value\">\r\n                    {{total_weight.value | round(3)}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
