 "html": "<style>\r\n    #qr.fixed {\r\n        position: fixed;\r\n        bottom: 25%;\r\n        left: 45%;\r\n        width: 175px;\r\n        height: 175px;\r\n        font-size: bold;\r\n    }\r\n\r\n    /*.qr-code {*/\r\n    /*    width: 200%;*/\r\n    /*    height: auto;*/\r\n    /*}*/\r\n\r\n    #totals {\r\n        position: fixed;\r\n        bottom: 25%;\r\n        right: 0%;\r\n        line-height: 1.8;\r\n        font-size: 12;\r\n    }\r\n    \r\n    #in-words {\r\n        position: fixed;\r\n        bottom: 25%;\r\n        left: 10%;\r\n        line-height: 1.8;\r\n        font-size: 12;\r\n    }\r\n    \r\n    #posting-date, #invoice-name {\r\n        position: relative;\r\n        top: -20px;\r\n    }\r\n    \r\n    #customer-name {\r\n        position: relative;\r\n        top: -30px;\r\n    }\r\n\r\n    /*.page-break {*/\r\n    /*    page-break-after: always;*/\r\n    /*}*/\r\n    .table {\r\n        width: 100%;\r\n    }\r\n    table.borderless td,table.borderless th {\r\n        border: none !important;\r\n        padding-top: -40px;\r\n    }\r\n    \r\n    @media print {\r\n\r\n    .page-break {\r\n        page-break-after: always;\r\n    }\r\n}\r\n\r\n</style>\r\n\r\n{%- set items_per_page = 10 -%}\r\n{%- set total_pages = ((doc.items|length / items_per_page)|round(0, 'ceil')|int) -%}\r\n{%- set cust_doc = frappe.get_doc(\"Customer\", doc.customer) -%}\r\n{%- for page in range(total_pages)-%}\r\n    <br>\r\n    <br>\r\n    \r\n    <div class=\"row\">\r\n        <div class=\"col-xs-4 text-left\" id=\"page-number\" style=\"text-align: right; text-left: 0px;font-size: 50;\">\r\n            <b>Page {{ page + 1 }} of {{ total_pages }}</b>\r\n        </div>\r\n    </div>\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-4 text-left\" id=\"page-number\" style=\"text-align: right; text-left: 0px;font-size: 50;\">\r\n            <b>{{cust_doc.custom_ref_no[-4:] if cust_doc.custom_ref_no else ''}}</b>\r\n        </div>\r\n    </div>\r\n    {%- if doc.custom_payment_type == \"Cash\" -%}\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-7 text-right\" id=\"type\">\r\n            <b>\u0641\u0627\u062a\u0648\u0631\u0629 \u0628\u064a\u0639 \u0646\u0642\u062f\u064a<br>\r\n            Cash Invoice</b>\r\n        </div>\r\n    </div>\r\n    {%- elif doc.custom_payment_type != \"Cash\" -%}\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-7 text-right\" id=\"type\">\r\n            <b>\u0641\u0627\u062a\u0648\u0631\u0629 \u0628\u064a\u0639<br>\r\n            Sales Invoice</b>\r\n        </div>\r\n    </div>\r\n    {%- endif -%}\r\n    \r\n    <div class=\"row\" id=\"posting-date\">\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align: left; text-left: 20px; font-size: 50\"><b>{{''}}</b></div>\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; font-size: 50;\">\r\n            <b>{{doc.posting_date or ''}}</b>\r\n        </div>\r\n    </div>\r\n    \r\n    <div class=\"row\" id=\"invoice-name\">\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; font-size: 50;\">\r\n            <b>{{ doc.name }}</b>\r\n        </div>\r\n        <div class=\"col-xs-10 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n    </div>\r\n    \r\n    <div class=\"row\" id=\"customer-name\">\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n        <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"text-align: right; font-size: 14px;\">\r\n            {% if doc.customer_name %}\r\n                <b>{{doc.custom_customer_name_en or doc.customer_name }}</b>\r\n            {% endif %}\r\n        </div>\r\n    </div>\r\n    <br>\r\n\r\n    <table class=\"table table-borderless\">\r\n    \t<tbody>\r\n    \t\t{%- for row in doc.items[page * items_per_page:(page + 1) * items_per_page] -%}\r\n    \t\t<tr>\r\n    \t\t\t<td style=\"width: 10%; text-align: left;\">\r\n    \t\t\t    <div style=\"border: 0px; \">\r\n    \t\t\t        <b>{{ row.item_code or '' }}</b>\r\n    \t\t\t    </div>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 50%; text-align: center;\">\r\n    \t\t\t\t<div style=\"border: 0px;\">\r\n    \t\t\t\t    <b>{{ row.description or '' }}</b>\r\n    \t\t\t\t</div>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 7.5%; text-align: center;\">\r\n    \t\t\t    <b>{{ row.uom }}</b>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 7.5%; text-align: center;\">\r\n    \t\t\t    <b>{{ row.qty | int }}</b>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 7.5%; text-align: center;\">\r\n    \t\t\t    <b>&nbsp;{{(\"%.3f\" % row.rate).split('.')[0]}}</b>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 5%; text-align: right;\">\r\n    \t\t\t    <b>{{ (\"%.3f\" % row.rate).split('.')[1]}}</b>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 4.5%; text-align: right;\">\r\n    \t\t\t    <b>&nbsp;&nbsp;&nbsp;{{(\"%.3f\" % row.amount).split('.')[0]}}</b>\r\n    \t\t\t</td>\r\n    \t\t\t<td style=\"width: 8%; text-align: right;\">\r\n    \t\t\t    <b>{{ (\"%.3f\" % row.amount).split('.')[1]}}</b>\r\n    \t\t\t</td>\r\n    \t\t</tr>\r\n    \t\t{%- endfor -%}\r\n    \t</tbody>\r\n    </table>\r\n    \r\n    {%- if not loop.last -%}\r\n    <div class=\"page-break\"></div>\r\n    {%- endif -%}\r\n    \r\n    <div class=\"ksa-vat-format col-xs-2 text-left fixed w3-container\" style=\"text-align: right; text-left: 0px;\" id=\"qr\">\r\n        <div class=\"qr-flex\">\r\n            <img class=\"qr-code\" src=\"{{doc.custom_qr_code_txt}}\">\r\n        </div>\r\n    </div>\r\n    {%- if loop.last -%}\r\n        {%- if loop.index == total_pages -%}\r\n            <div id=\"totals\" class=\"fixed w3-container\">\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>{{ (\"%.3f\" % doc.total).split('.')[0] }}</b> &nbsp;  <b>{{ (\"%.3f\" % doc.total).split('.')[1] or '0' }}</b>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>{{ (\"%.3f\" % doc.discount_amount).split('.')[0] }}</b> &nbsp;  <b>{{ (\"%.3f\" % doc.discount_amount).split('.')[1] or '0' }}</b>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>{{ (\"%.3f\" % doc.net_total).split('.')[0] }}</b> &nbsp;  <b>{{ (\"%.3f\" % doc.net_total).split('.')[1] or '0' }}</b>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>{{ (\"%.3f\" % doc.total_taxes_and_charges).split('.')[0] }}</b> &nbsp;  <b>{{ (\"%.3f\" % doc.total_taxes_and_charges).split('.')[1] or '0' }}</b>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12 text-right\">\r\n                        <b>{{ (\"%.3f\" % doc.grand_total).split('.')[0] }}</b> &nbsp;  <b>{{ (\"%.3f\" % doc.grand_total).split('.')[1] or '0' }}</b>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div id=\"in-words\" class=\"fixed w3-container\">\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-16 text-left\"><b>{{ frappe.utils.money_in_words(doc.grand_total, \"JOD\")}}</b></div>\r\n                </div>\r\n            </div>\r\n        {%- endif -%}\r\n    {%- endif -%}\r\n{%- endfor -%}",
