 "css": ".account-info-row {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%; \r\n    margin-bottom: 8px;\r\n    align-items: center;\r\n    /*border-bottom: 1px Solid #bcbcbc;*/\r\n    font-weight: bold;\r\n}\r\n.account-info-row span {\r\n    /*flex: 1;*/\r\n    text-align: center;\r\n}\r\n.label-right {\r\n    text-align: right;\r\n    font-weight: bold;\r\n    direction: rtl;\r\n    /*min-width: 80px;*/\r\n}\r\n.label-left {\r\n    font-weight: bold;\r\n    /*min-width: 80px;*/\r\n}",
 "custom_format": 1,
 "default_print_language": "en-US",
 "disabled": 0,
 "doc_type": "Journal Entry",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<div style=\"font-family: Arial, sans-serif; border: 2px solid #000; padding: 20px; max-width: 800px;\">\r\n    <!-- Header -->\r\n    <h4>\r\n        <div style=\"text-align: center;\">\r\n            <div style=\"font-size: 14px; margin-bottom: 5px;\">\u0625\u0634\u0639\u0627\u0631 \u0642\u064a\u062f \u062f\u0627\u0626\u0646</div>\r\n            <div style=\"font-size: 14px; font-weight: bold;\">Credit Note</div>\r\n        </div>\r\n    </h4>\r\n    <!-- Account Info -->\r\n    <div class=\"account-info-row\">\r\n        <span class=\"label-left\">Number:</span>\r\n        <span class=\"value\">{{ doc.custom_reference_no }}</span>\r\n        <span class=\"label-right\">\u0631\u0642\u0645 \u0627\u0644\u0625\u0634\u0639\u0627\u0631:</span>\r\n    </div>\r\n    <div class=\"account-info-row\">\r\n        <span class=\"label-left\">Date:</span>\r\n        <span class=\"value\">{{ doc.posting_date }}</span>\r\n        <span class=\"label-right\">\u0627\u0644\u062a\u0627\u0631\u064a\u062e:</span>\r\n    </div>\r\n    <div class=\"account-info-row\">\r\n        <span class=\"label-left\">A/C Name:</span>\r\n        <span class=\"value\">{{ doc.accounts[0].party or \"\" }}</span>\r\n        <span class=\"label-right\">\u0627\u0633\u0645 \u0627\u0644\u062d\u0633\u0627\u0628:</span>\r\n    </div>\r\n    <div class=\"account-info-row\">\r\n        <span class=\"label-left\">A/C No.:</span>\r\n        <span class=\"value\">{{ frappe.db.get_value(\"Customer\", doc.accounts[0].party, \"custom_ref_no\") or \"\" }}</span>\r\n        <span class=\"label-right\">\u0631\u0642\u0645 \u0627\u0644\u062d\u0633\u0627\u0628:</span>\r\n    </div>\r\n    <div class=\"account-info-row\">\r\n        <span class=\"label-left\">Address:</span>\r\n        <span class=\"value\">\r\n            {% if doc.accounts[0].party %}\r\n                {% set customer_details = frappe.db.get_value(\"Customer\", doc.accounts[0].party, [\"customer_primary_address\", \"primary_address\"], as_dict=1) %}\r\n                {% if customer_details and customer_details.customer_primary_address %}\r\n                    {% set address = frappe.db.get_value(\"Address\", customer_details.customer_primary_address, [\"address_line1\", \"address_line2\", \"city\", \"state\", \"country\"], as_dict=1) %}\r\n                    {% if address %}\r\n                        {{ address.address_line1 or \"\" }}\r\n                        {% if address.city %}{{ \", \" + address.city }}{% endif %}\r\n                    {% endif %}\r\n                {% endif %}\r\n            {% endif %}\r\n        </span>\r\n        <span class=\"label-right\">\u0627\u0644\u0639\u0646\u0648\u0627\u0646:</span>\r\n    </div>\r\n\r\n    <!-- Notice Section -->\r\n    <div style=\"border: 2px solid #000; padding: 10px; margin-bottom: 20px;\">\r\n        <div style=\"display: flex; justify-content: space-between; align-items: center;\">\r\n            <div style=\"width: 48%;\">\r\n                <strong>Please Note that we have Credited your Account With the Following:</strong>\r\n            </div>\r\n            <div style=\"width: 48%; text-align: right; direction: rtl;\">\r\n                <strong>\u0646\u062d\u064a\u0637\u0643\u0645 \u0639\u0644\u0645\u0627 \u0628\u0623\u0646\u0646\u0627 \u0642\u064a\u062f\u0646\u0627 \u0644\u062d\u0633\u0627\u0628\u0643\u0645 \u0645\u0627 \u064a\u0644\u064a:</strong>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Transaction Details Table -->\r\n    <table style=\"width: 100%; border: 2px solid #000; border-collapse: collapse; margin-bottom: 20px;\">\r\n        <thead>\r\n            <tr style=\"background-color: #f0f0f0;\">\r\n                <th style=\"border: 1px solid #000; padding: 8px; width: 25%; text-align: center;\">\r\n                    <div>\u0627\u0644\u0645\u0628\u0644\u063a \u0628\u0627\u0644\u062f\u064a\u0646\u0627\u0631 \u0627\u0644\u0623\u0631\u062f\u0646\u064a</div>\r\n                    <div>Amount (JOD)</div>\r\n                </th>\r\n                <th style=\"border: 1px solid #000; padding: 8px; width: 15%; text-align: center;\">Fils</th>\r\n                <th style=\"border: 1px solid #000; padding: 8px; width: 60%; text-align: center;\">\r\n                    <div>DESCRIPTION</div>\r\n                    <div style=\"text-align: center; direction: rtl;\">\u0627\u0644\u0628\u064a\u0627\u0646</div>\r\n                </th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            {% for acc in doc.accounts %}\r\n                {% if acc.credit_in_account_currency %}\r\n                {% set amount = acc.credit_in_account_currency or acc.debit_in_account_currency %}\r\n                {% set jd_amount = (amount|float)|int %}\r\n                {% set fils_amount = ((amount|float - jd_amount) * 1000)|int %}\r\n                <tr>\r\n                    <td style=\"border: 1px solid #000; padding: 8px; text-align: center;\">\r\n                        {{ jd_amount }}\r\n                    </td>\r\n                    <td style=\"border: 1px solid #000; padding: 8px; text-align: center;\">\r\n                        {{ fils_amount }}\r\n                    </td>\r\n                    <td style=\"border: 1px solid #000; padding: 8px;\">\r\n                        {{ doc.user_remark or doc.remark or \"\" }}\r\n                    </td>\r\n                </tr>\r\n                {% endif %}\r\n            {% endfor %}\r\n            <!-- Add empty rows -->\r\n            {% for i in range(5) %}\r\n            <tr>\r\n                <td style=\"border: 1px solid #000; padding: 15px;\">&nbsp;</td>\r\n                <td style=\"border: 1px solid #000; padding: 15px;\">&nbsp;</td>\r\n                <td style=\"border: 1px solid #000; padding: 15px;\">&nbsp;</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n    {% set words = frappe.utils.money_in_words(doc.total_credit, \"JOD\") %}\r\n    <!-- Footer sections -->\r\n    <div style=\"margin-bottom: 15px;\">\r\n        <strong>Only the Amount of JOD:</strong>\r\n        {{ _(words) }}\r\n        <span style=\"float: right; direction: rtl;\"><strong>\u0641\u0642\u0637 \u0644\u0627 \u063a\u064a\u0631:</strong></span>\r\n    </div>\r\n\r\n    <div style=\"display: flex; justify-content: space-between; margin-bottom: 20px;\">\r\n        <div></div>\r\n        <div style=\"text-align: right; direction: rtl;\">\r\n            <strong>\u0627\u0644\u0645\u062d\u0627\u0633\u0628:</strong>\r\n            <span style=\"display: inline-block; border-bottom: 1px solid #000; min-width: 200px;\">&nbsp;</span>\r\n        </div>\r\n    </div>\r\n\r\n    <div style=\"text-align: center; direction: rtl; font-weight: bold;\">\r\n        \u0631\u0627\u062c\u064a\u0646 \u0627\u062c\u0631\u0627\u0621 \u0627\u0644\u0642\u064a\u062f \u0627\u0644\u0644\u0627\u0632\u0645 \u0639\u0644\u0649 \u062d\u0633\u0627\u0628\u0646\u0627 \u0644\u062f\u064a\u0643\u0645\r\n    </div>\r\n</div>",
