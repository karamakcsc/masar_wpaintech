 "html": "<div class=\"page-break\">\n    <div class=\"print-heading\">\n        <h2>Stock Entry<br>\n            <small>{{doc.name}}</small>\n        </h2>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Stock Entry Type:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{doc.stock_entry_type}}\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Posting Date:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{frappe.utils.formatdate(doc.posting_date)}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <!--<br>-->\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Purpose:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{doc.purpose}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"margin-top\">\n        <table class='table table-bordered table-condensed'>\n            <tr>\n                <th>Sr.</th>\n                <th>Source Warehouse</th>\n                <th>Target Warehouse</th>\n                <th>Item Code</th>\n                <th>Item Group</th>\n                <th>Description</th>\n                <th>UOM</th>\n                <th>Qty</th>\n                <th>Valuation Rate</th>\n                <th>Amount</th>\n            </tr>\n            {% set total_amount = namespace(value=0) %}\n            {% for row in doc.items %}\n            {% set total_amount.value = total_amount.value + row.amount %}\n            <tr>\n                <td>{{ row.idx }}</td>\n                <td>{{ row.s_warehouse or '' }}</td>\n                <td>{{ row.t_warehouse or '' }}</td>\n                <td>{{ row.item_code }}</td>\n                <td>{{ row.item_group }}</td>\n                <td style=\"width: 30%;\">{{ row.description }}</td>\n                <td>{{ row.uom }}</td>\n                <td>{{ row.qty | int }}</td>\n                <td>{{ row.valuation_rate | round(3) }}</td>\n                <td>{{ row.amount | round(3) }}</td>\n            </tr>\n            {% endfor %}\n        </table>\n    </div>\n    <div class=\"col-xs-6 column-break\">\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\">\n                <label>Total Value Difference (Incoming - Outgoing):</label>\n            </div>\n            <div class=\"col-xs-7 text-right value\">\n                {{doc.value_difference | round(3)}}\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\">\n                <label>Total Amount:</label>\n            </div>\n            <div class=\"col-xs-7 text-right value\">\n                {{total_amount.value | round(3)}}\n            </div>\n        </div>\n    </div>   \n</div>",
