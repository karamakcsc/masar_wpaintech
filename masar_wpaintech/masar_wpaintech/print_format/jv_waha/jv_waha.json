 "html": "<div class=\"page-break\">\n    <div class=\"print-heading\">\n        <!--<div class=\"letter-head\">{{ letter_head }}</div>-->\n        <h2>Journal Entry<br>\n            <small>{{doc.name}}</small>\n        </h2>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Entry Type:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{doc.voucher_type}}\n                </div>\n            </div>\n        </div>\n        \n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Reference No:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{doc.custom_reference_no}}\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Posting Date:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{frappe.utils.formatdate(doc.posting_date)}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"margin-top\">\n        <table class='table table-bordered'>\n            <tr>\n                <th>Sr.</th>\n                <th>Account</th>\n                <th>Party Type</th>\n                <th>Party</th>\n                <th>Debit</th>\n                <th>Credit</th>\n                <th>Remarks</th>\n            </tr>\n            {% for entries in doc.accounts %}\n                <tr>\n                    <td>{{ loop.index }}</td>\n                    <td>{{ entries.account }}</td>\n                    <td>{{ entries.party_type }}</td>\n                    <td>{{ entries.party }}</td>\n                    <td>{{ entries.debit | round(3) }}</td>\n                    <td>{{ entries.credit | round(3) }}</td>\n                    <td>{{ entries.user_remark }}</td>\n                </tr>\n            {% endfor %}\n        </table>\n    </div>\n    {% if doc.user_remark %}\n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Remark:</label>\n                </div>\n                <div class=\"col-xs-7 value\">\n                    {{ doc.user_remark }}\n                </div>\n            </div>\n        </div>\n    {% endif %}\n</div>",
